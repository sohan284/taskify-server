{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "MONGO_URI": "@mongo-uri",
      "PORT": "5000"
    },
    "functions": {
      "server.js": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }
  